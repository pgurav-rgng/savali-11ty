  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "npx tailwindcss -i ./public/input.css -o ./public/output.css --minify",
    "watch:css": "npx tailwindcss -i ./public/input.css -o ./public/output.css --watch",
    "build:eleventy": "eleventy",
    "serve:eleventy": "eleventy --serve",
    "build": "npm run build:css && npm run build:eleventy",
    "start": "npm run build:css && concurrently \"npm run watch:css\" \"npm run serve:eleventy\"",
    "dev": "concurrently \"npx tailwindcss -i public/input.css -o public/output.css --watch\" \"npx @11ty/eleventy --watch\""
  },
